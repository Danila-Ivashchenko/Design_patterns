ной грудкой и можно подавать.'] \par\par\b Recipe 3:\b0 \par\b id:\b0 8a188bb8-c52e-4364-8ff2-9be5d70adf0a \par\b ingredients:\b0 [{'id': 'd831d1b3-d1d9-40c1-a742-8a26ed4325fb', 'measurement_value': {'id': 'e30ce288-6944-4b02-bc36-42ed710b0b5c', 'unit': {'id': '3e07673a-c639-45f7-a29b-9b0ac0d05ec2', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '150.0'}, 'nomenclature': {'id': '21fe5a5a-a553-4b9d-99cc-5347228551ef', 'measurement_uint': {'id': '731f1c94-ecd0-4739-945f-890a09e9f2f1', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'пшеничная  мука', 'nomenclature_group_id': '49185df1-8f62-4a97-a935-4603e8beec7c'}}, {'id': 'b2cb4cbf-3b33-4a9e-8f45-f726674abf15', 'measurement_value': {'id': 'c96b14d5-79de-4d4a-9a29-21dbeac00205', 'unit': {'id': '3e07673a-c639-45f7-a29b-9b0ac0d05ec2', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '60.0'}, 'nomenclature': {'id': '7fe4e68f-11fc-4411-b8db-65732a754b15', 'measurement_uint': {'id': '731f1c94-ecd0-4739-945f-890a09e9f2f1', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'сахар', 'nomenclature_group_id': '49185df1-8f62-4a97-a935-4603e8beec7c'}}, {'id': '31c6fcfa-9d7f-41aa-a1d1-18ed8596a0e6', 'measurement_value': {'id': 'd58c4d23-3e4f-4edf-ad2e-2283956abeee', 'unit': {'id': '3e07673a-c639-45f7-a29b-9b0ac0d05ec2', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '75.0'}, 'nomenclature': {'id': '0b8f7265-f093-429d-b87d-ef6e4ae18a03', 'measurement_uint': {'id': '731f1c94-ecd0-4739-945f-890a09e9f2f1', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'масло', 'nomenclature_group_id': '49185df1-8f62-4a97-a935-4603e8beec7c'}}, {'id': '0a8c5072-d05c-4a15-9b92-691f03fd92c8', 'measurement_value': {'id': '466c8b41-a247-487f-8534-092a4adc59fa', 'unit': {'id': '759b570b-4747-49aa-b61e-fcf689988b83', 'name': 'штука', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '2.0'}, 'nomenclature': {'id': '41846191-986a-4415-83c5-44e435eb72b3', 'me