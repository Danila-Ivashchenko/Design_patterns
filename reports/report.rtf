куриной грудкой и можно подавать.'] \par\par\b Recipe 3:\b0 \par\b id:\b0 2d65b882-2da1-4182-b5be-c5486496c6b8 \par\b ingredients:\b0 [{'id': '89caa3ef-ba11-40df-a137-9e797b802097', 'measurement_value': {'id': 'f785a2ad-c899-4098-88a9-c7c3e366fad5', 'unit': {'id': 'e7b83c31-bcea-4882-9dc4-72a1fba1d39f', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '150.0'}, 'nomenclature': {'id': 'de485644-4955-4239-a32b-bad046f2a84d', 'measurement_uint': {'id': 'f2ac2a6a-ab8b-4e77-8c54-509025f55b47', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'пшеничная  мука', 'nomenclature_group_id': '68397aca-93b5-4ebb-9223-1f6ff696e95e'}}, {'id': '80ec2ea6-e6ec-452d-ae13-085dab9e02ad', 'measurement_value': {'id': 'c6ca9868-2ed4-42a1-b4fa-7645dab0fda4', 'unit': {'id': 'e7b83c31-bcea-4882-9dc4-72a1fba1d39f', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '60.0'}, 'nomenclature': {'id': '3d2b3ea3-1720-4171-b02d-83cd880804df', 'measurement_uint': {'id': 'f2ac2a6a-ab8b-4e77-8c54-509025f55b47', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'сахар', 'nomenclature_group_id': '68397aca-93b5-4ebb-9223-1f6ff696e95e'}}, {'id': '001d8ea5-96cf-484c-a841-9f340a9d93f5', 'measurement_value': {'id': 'f1797e0c-ec4d-4ffe-a576-687e1e7726f3', 'unit': {'id': 'e7b83c31-bcea-4882-9dc4-72a1fba1d39f', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '75.0'}, 'nomenclature': {'id': 'b62ce205-5084-49ad-913a-00c3eae58046', 'measurement_uint': {'id': 'f2ac2a6a-ab8b-4e77-8c54-509025f55b47', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'масло', 'nomenclature_group_id': '68397aca-93b5-4ebb-9223-1f6ff696e95e'}}, {'id': '7eaef621-8ba2-42a9-abc9-3bf15a868eb9', 'measurement_value': {'id': 'bc53cdd6-d9a6-4ba0-9412-1fd71ba7ea7f', 'unit': {'id': 'fc40a371-208a-4e9b-8a2b-867ff0f65311', 'name': 'штука', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '2.0'}, 'nomenclature': {'id': 'a0bc7fd7-076e-441b-8558-e2a15edd2cc8',