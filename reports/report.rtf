ной грудкой и можно подавать.'] \par\par\b Recipe 3:\b0 \par\b id:\b0 40a9d3ab-632a-4407-86de-e9aabb070bca \par\b ingredients:\b0 [{'id': '6870c3b4-0121-44c5-8637-f8a5fef475fa', 'measurement_value': {'id': '84df679b-34c5-4e9c-94a0-360a5e1d843a', 'unit': {'id': '880656a0-b590-46ad-af9a-7e438a7dcdaa', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '150.0'}, 'nomenclature': {'id': '548028dd-b261-4cfa-a1bd-ab89bab85882', 'measurement_uint': {'id': '550db0e9-f0ff-4382-9f3b-3aba0f0a4928', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'пшеничная  мука', 'nomenclature_group_id': '9721052c-24c5-4d82-b63f-b4d19c00dec1'}}, {'id': '9fa614aa-c780-472d-b1c7-1ab4dfd66ff9', 'measurement_value': {'id': '6ebec2cd-29f0-45f4-ab9c-45678183628a', 'unit': {'id': '880656a0-b590-46ad-af9a-7e438a7dcdaa', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '60.0'}, 'nomenclature': {'id': 'eeefc6a5-cadd-4ff7-8036-8c63884290bf', 'measurement_uint': {'id': '550db0e9-f0ff-4382-9f3b-3aba0f0a4928', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'сахар', 'nomenclature_group_id': '9721052c-24c5-4d82-b63f-b4d19c00dec1'}}, {'id': 'e1be0334-6aae-49b6-b3eb-28ed70642608', 'measurement_value': {'id': '2cd6ea75-8497-4c53-9950-734634f758c7', 'unit': {'id': '880656a0-b590-46ad-af9a-7e438a7dcdaa', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '75.0'}, 'nomenclature': {'id': '4045c172-2976-454e-8b90-3e5af972a5c2', 'measurement_uint': {'id': '550db0e9-f0ff-4382-9f3b-3aba0f0a4928', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'масло', 'nomenclature_group_id': '9721052c-24c5-4d82-b63f-b4d19c00dec1'}}, {'id': '4ffa28b6-a305-4cb9-8b34-7734e7890b6a', 'measurement_value': {'id': '39b3f929-b4bb-4bfa-850b-580d42bc3820', 'unit': {'id': '337632a5-dc55-4dba-b8ba-3673c4db3112', 'name': 'штука', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '2.0'}, 'nomenclature': {'id': '39519ec3-de22-4c36-a265-bd52cb511285', 'me