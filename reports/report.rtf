ной грудкой и можно подавать.'] \par\par\b Recipe 3:\b0 \par\b id:\b0 ee035727-3e3f-4286-9a07-047dd6314751 \par\b ingredients:\b0 [{'id': '578494f8-e58d-4934-8708-e61d849fa1cc', 'measurement_value': {'id': '33893b7d-86ea-4440-903a-f05f9100273e', 'unit': {'id': 'eab0b215-f225-4f82-9dae-14cc5fd303b6', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '150.0'}, 'nomenclature': {'id': '9fad9db7-df53-4619-816a-4f014ad70fc9', 'measurement_uint': {'id': 'edd8ef22-a2ac-455d-b68c-7a01ba4e9a33', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'пшеничная  мука', 'nomenclature_group_id': '945a4893-3762-4cdb-97fb-da181448bc82'}}, {'id': '86365fb0-3a75-487c-b6fb-269f4263f09d', 'measurement_value': {'id': 'e1fb292f-b441-4119-bdb2-6d146676523e', 'unit': {'id': 'eab0b215-f225-4f82-9dae-14cc5fd303b6', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '60.0'}, 'nomenclature': {'id': 'c3055f2c-d745-4f85-93dd-eecb58574c29', 'measurement_uint': {'id': 'edd8ef22-a2ac-455d-b68c-7a01ba4e9a33', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'сахар', 'nomenclature_group_id': '945a4893-3762-4cdb-97fb-da181448bc82'}}, {'id': 'a4d4a493-d497-46fc-aa53-d6f4395ffa25', 'measurement_value': {'id': '8530a4f0-72d1-4d28-b427-eda234255ca4', 'unit': {'id': 'eab0b215-f225-4f82-9dae-14cc5fd303b6', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '75.0'}, 'nomenclature': {'id': '68ea739e-e7c2-4ad7-bda5-3a5a69ac27a5', 'measurement_uint': {'id': 'edd8ef22-a2ac-455d-b68c-7a01ba4e9a33', 'name': 'грамм', 'parent_unit': 'None', 'ratio': '1.0'}, 'name': 'масло', 'nomenclature_group_id': '945a4893-3762-4cdb-97fb-da181448bc82'}}, {'id': '1040d78c-09ad-4409-9cdb-214c02fc5902', 'measurement_value': {'id': '69a533fa-d674-4990-8f11-6ee8d5caed2d', 'unit': {'id': '03f78806-e03c-4d8a-983c-b43ec69d53f7', 'name': 'штука', 'parent_unit': 'None', 'ratio': '1.0'}, 'value': '2.0'}, 'nomenclature': {'id': '8aa73f1f-b773-49ce-bbfb-43c382973b82', 'me